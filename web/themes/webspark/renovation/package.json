{
    "name": "renovation",
    "description": "A theme based on Radix.",
    "private": true,
    "scripts": {
        "postinstall": "find node_modules/ -name '*.info' -type f -delete && cp ./config.js ./config.local.js",
        "dev": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "production": "mix --production"
    },
    "devDependencies": {
        "bootstrap": "5.3.0",
        "browser-sync": "^2.29.3",
        "browser-sync-webpack-plugin": "^2.3.0",
        "cross-env": "5.2.1",
        "jquery": "^3.5.1",
        "laravel-mix": "^6.0.49",
        "sass": "^1.65.1",
        "sass-loader": "^12.1.0"
    },
    "dependencies": {
        "@asu/unity-bootstrap-theme": "1.20.2",
        "@popperjs/core": "^2.11.8"
    }
}
